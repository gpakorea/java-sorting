// SortArray.java

public class SortArray() {
  
  // instance variables
  int[] array = { 76, 71, 63, 65, 66, 64, 70, 69, 68, 67, 72, 73 };
  
  // constructor
  public SortArray() {
    
  }
  
  // methods
  public void toString() {
    
    System.out.print( "{ " );
    for( int i = 0; i < array.length-1; i++ ) {
      System.out.print( array[i] + ", " );
    }
    System.out.print( array[array.length] + " }" );
    
  } // END toString
  
} // END class